<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="423.0" prefWidth="554.0" style="-fx-background-color: Thistle;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="process.controller.graphic.GraphicMenu">
    <children>
        <HBox prefHeight="70.0" prefWidth="554.0">
         <children>
            <MenuBar prefHeight="30.0" prefWidth="554.0">
              <menus>
                <Menu mnemonicParsing="false" text="Выход">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#comeBack" text="Веруться в меню" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
        </HBox>
        <AreaChart fx:id="areaChartOnRequestFromCountry" prefHeight="485.0" prefWidth="554.0" style="-fx-cursor: black; -fx-legend-side: Red; -fx-background-color: Thistle;" title="График заработанной суммы в городе">
            <xAxis>
                <CategoryAxis side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT" />
            </yAxis>
        </AreaChart>
    </children>
</VBox>
