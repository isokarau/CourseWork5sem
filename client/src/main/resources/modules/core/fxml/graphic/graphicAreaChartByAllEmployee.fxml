        <HBox prefHeight="70.0" prefWidth="554.0">
            <children>
                <MenuBar prefHeight="30.0" prefWidth="554.0">
                    <menus>
                        <Menu mnemonicParsing="false" text="Выход">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#comeBack" text="Веруться в меню" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </children>
        </HBox>
        <AreaChart fx:id="areaChartOnRequestFromCountry" prefHeight="485.0" prefWidth="554.0" style="-fx-cursor: black; -fx-legend-side: Red; -fx-background-color: Thistle;" title="Прибыль копании в текущем году">
